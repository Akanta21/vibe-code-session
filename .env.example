# Telegram Bot Configuration
# Get bot token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Admin chat ID where registrations will be sent
# Can be your user ID or a group chat ID
TELEGRAM_ADMIN_CHAT_ID=your_chat_id_here

# Your domain for webhook (when deployed)
WEBHOOK_DOMAIN=https://yourdomain.com

# Email Configuration (Resend)
# Get API key from resend.com
RESEND_API_KEY=re_your_resend_api_key_here

# Email sender (use your verified domain)
EMAIL_FROM=Vibe Coding <noreply@yourdomain.com>

# Optional: Paynow UEN for payment QR generation
PAYNOW_UEN=your_paynow_uen_here